  "required": [
    "type",
    "items"
  ],
  "properties": {
    "type": {
      "const": "list"
    },
    "items": {
      "type": "object",
      "required": [
        "type"
      ],
      "properties": {
        "type": {
          "type": "string"
        }
      }
    }
  }
}