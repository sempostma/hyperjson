{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Hojui builtin number",
  "type": "object",
  "required": ["items"],
  "properties": {
    "items": {
      "type": "object",
      "required": [
        "name",
        "child"
      ],
      "properties": {
        "name": {
          "type": "string"
        },
        "key": {
          "type": "string"
        },
        "child": {
          "type": "object",
          "required": [
            "type",
            "schema"
          ],
          "properties": {
            "type": {
              "type": "string"
            },
            "schema": {
              "type": "object",
              "additionalProperties": true
            }
          }
        }
      }
    }
  }
}